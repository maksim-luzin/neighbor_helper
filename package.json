{
  "name": "neighbor_helper",
  "version": "1.0.0",
  "files": [
    "bin"
  ],
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.js --exec \"npm run linter && babel-node\"",
    "linter": "eslint src",
    "bab": "babel src -d bin",
    "gulp": "./node_modules/.bin/gulp",
    "claudia": "./node_modules/.bin/claudia",
    "transpile": "npm run linter && gulp",
    "create": "npm run transpile && claudia create --profile claudia --region eu-central-1  --api-module bin/app",
    "update": "npm run transpile && claudia update --profile claudia",
    "registrate": "npm run transpile && claudia update --profile claudia --configure-telegram-bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paratskiy-organization/neighbor_helper.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "claudia": "^5.12.0",
    "core-js": "^3.6.5",
    "del": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-uglify-es": "^2.0.0",
    "nodemon": "^2.0.6",
    "regenerator-runtime": "^0.13.7"
  },
  "dependencies": {
    "claudia-bot-builder": "^4.5.0"
  },
  "bugs": {
    "url": "https://github.com/paratskiy-organization/neighbor_helper/issues"
  },
  "homepage": "https://github.com/paratskiy-organization/neighbor_helper#readme"
}
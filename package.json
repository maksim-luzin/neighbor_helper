{
  "name": "neighbor_helper",
  "version": "1.0.0",
  "files": [
    "bin"
  ],
  "description": "",
  "main": "bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/bot.js --exec \"npm run lint\"",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "eslintLocal": "eslint ./local.js",
    "debug": "nodemon ./local.js --exec \"npm run eslintLocal && node\"",
    "gulp": "./node_modules/.bin/gulp",
    "transpile": "npm run lint && gulp",
    "claudia": "./node_modules/.bin/claudia",
    "create": "npm run transpile && claudia create --profile ChatbotsStudioInternship --region eu-central-1  --api-module bin/bot",
    "registrate": "npm run transpile && claudia update --profile ChatbotsStudioInternship --configure-telegram-bot",
    "update": "npm run transpile && claudia update --profile ChatbotsStudioInternship"
  },
  "dependencies": {
    "claudia-bot-builder": "^4.5.0",
    "dotenv": "^8.2.0",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-replace-enum-postgres": "^1.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@types/sequelize": "^4.28.9",
    "@types/winston": "^2.4.4",
    "axios": "^0.21.0",
    "claudia": "^5.12.0",
    "del": "^6.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-uglify-es": "^2.0.0",
    "ngrok": "^3.3.0",
    "nodemon": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paratskiy-organization/neighbor_helper.git"
